#!/usr/bin/env bash

for r in aliases/* functions/* get_binaries options vars/* liquidpromptrc liquidprompt ; do
  source ${r}
done

# load vault
[ -f ~/.vaultrc ] && source ~/.vaultrc |& logger -t vaultrc

# Source extra Mac stuff if on a Mac
[[ "$(uname)" == 'Darwin' ]] && source "$PWD/osx"

prompt_on
# PROMPT_COMMAND="promptline"
